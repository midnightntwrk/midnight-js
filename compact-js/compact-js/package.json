{
  "name": "@midnight-ntwrk/compact-js",
  "version": "1.0.0",
  "description": "Typescript-based execution environment for smart contracts compiled with the Compact language",
  "type": "module",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs"
    },
    "./effect": {
      "types": "./dist/effect/index.d.ts",
      "import": "./dist/effect/index.mjs"
    }
  },
  "dependencies": {
    "@effect/platform": "^0.89.0",
    "@midnight-ntwrk/compact-runtime": "0.9.0-rc.0",
    "@midnight-ntwrk/ledger": "5.0.0-alpha.3",
    "effect": "^3.16.16"
  },
  "devDependencies": {
    "@effect/vitest": "^0.24.1"
  },
  "repository": "git@github.com:midnight-ntwrk/artifacts",
  "packageManager": "yarn@4.9.2",
  "author": "IOHK",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rm -rf dist tsconfig.build.tsbuildinfo .rollup.cache reports coverage",
    "build": "tsc -b tsconfig.build.json",
    "test": "vitest run",
    "deploy": "yarn npm publish"
  },
  "files": [
    "dist/"
  ]
}
